/**app.wxss**/
@import './font/iconfont.wxss';

.container {
 color: #333;
  background-color: #f5f5f9;
  min-height: 100%;
  box-sizing: border-box;
  position: relative;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif, PingFang-SC-Medium, 'Heiti SC';
} 
view,
image,
text,
navigator {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

[class^="icon-"]:before {
  font-family: iconfont;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
}